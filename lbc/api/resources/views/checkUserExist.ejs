


<%if(checkEmail){%>
    <p class="text-red-500">Người dùng này chính là bản thân bạn</p>
    <input type="hidden" id="checkUser" value="false">
<%}else{%>
    <% if(listUser.length > 0){ %>
        <p class="text-green-500">Họ tên: <%= listUser[0].fullname %></p>
        <p class="text-green-500">CMND/CCCD:  <%= listUser[0].idCard %></p>
        <input type="hidden" id="checkUser" value="true">
        <input type="hidden" id="name" value=<%= listUser[0].fullname %>>
        <!-- Modal -->
        <div class="modal fade text-black" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Xác nhận chuyển nhượng</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                    <label for="" id="showNumberPhone">Nhập OTP đã gửi về số điện thoại : </label>
                    &emsp; 
                    <input id="code" type="text" class="form-control w-64" placeholder="Nhập OTP"><br>
                    <button id="submitOTP" type="submit" class="btn btn-primary">Xác nhận</button>
                    &emsp; 
                    </div>
            
                </div>
            </div>
            </div>
            </div>
    <% }else{ %>
        <p class="text-red-500">Không có người dùng này</p>
        <input type="hidden" id="checkUser" value="false">
    <% } %>

<%}%>

