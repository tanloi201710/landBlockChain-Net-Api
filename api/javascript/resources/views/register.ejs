    <div class="limiter">
		<div class="container-login100" style="background-image: url('images/bg-01.jpg');">
        <div class="wrap-login100 p-t-30 p-b-50">
				<span class="login100-form-title p-b-41">
					Account Register
				</span>
				<%
						if(error.length > 0){
					%>
					<div id="error" class="alert alert-success bg-danger text-white" style="text-align:center;">
						
										<%= error%>
						
					</div>
					<%
									}
								%>

				<form id="registerForm" class="login100-form validate-form p-b-33 p-t-5" method="post" action="/handleRegister"  onsubmit="return false;">
					
					<div class="wrap-input100 validate-input">
						<input class="input100" type="text" id="email" name="email" placeholder="Email">
						<span class="focus-input100" data-placeholder="&#xe818;"></span>
					</div>

					<div class="wrap-input100 validate-input">
						<input class="input100" type="password" id="password" name="password" placeholder="Mật khẩu">
						<span class="focus-input100" data-placeholder="&#xe80f;"></span>
					</div>

					<div class="wrap-input100 validate-input">
						<input class="input100" type="password" id="passwordR" name="passwordR" placeholder="Nhập lại mật khẩu">
						<span class="focus-input100" data-placeholder="&#xe80f;"></span>
					</div>

                    <div class="wrap-input100 validate-input">
						<input class="input100" type="text" id="fullname" name="fullname" placeholder="Họ tên">
						<span class="focus-input100" data-placeholder="&#xe82a;"></span>
					</div>

                    <div class="wrap-input100 validate-input">
						<input class="input100" type="text" id="idCard" name="idCard" placeholder="CMND/CCCD">
						<span class="focus-input100" data-placeholder="&#xe800;"></span>
					</div>

                    <div class="wrap-input100 validate-input" >
						<input class="input100" type="text" id="numberPhone" name="numberPhone" placeholder="Số điện thoại">
						<span class="focus-input100" data-placeholder="&#xe830;"></span>
					</div>
                    <!-- <div class="wrap-input100 validate-input" > -->
						<!-- <input class="input100" type="text" id="otp" name="otp" placeholder="Enter OTP">
						<span class="focus-input100" data-placeholder="&#xe830;"></span> -->
					<!-- </div> -->
                    

                    <div id="recaptcha-container"></div>

                    <div class="container-login100-form-btn m-t-32 d-flex justify-content-around" > 
						<button class="login100-form-btn" id="registerButton">
							Đăng ký
						</button>
					</div>
					<br>
						<div class="d-flex justify-content-center">
						
						<a href="/login" class="text-info">Đã có tài khoản</a>
					</div>
					<br>
					<div id="error"  class="alert alert-success" style="text-align:center; display: none;">
						
					</div>
				</form>

				
			</div>
  
